)),\r\n  startGenerateForm : (surveyData)=> dispatch(startGeneratingSurveyAsync(surveyData))\r\n\r\n});\r\n\r\nexport default compose(\r\n  connect(mapStateToProps, mapDispatchToProps),\r\n  withRouter\r\n  )(StepperComponent)\r\n\r\n","D:\\projects\\Node\\Udemy Node\\server\\client\\src\\components\\surveys\\display-user-from-data\\ShowUserFormdataComponent.jsx",[],"D:\\projects\\Node\\Udemy Node\\server\\client\\src\\components\\user-response\\UserResponse.component.jsx",[],"D:\\projects\\Node\\Udemy Node\\server\\client\\src\\components\\profile\\user-profile.component.jsx",[],{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","severity":1,"message":"115","line":7,"column":8,"nodeType":"116","messageId":"117","endLine":7,"endColumn":21},{"ruleId":"114","severity":1,"message":"118","line":28,"column":45,"nodeType":"116","messageId":"117","endLine":28,"endColumn":65},{"ruleId":"114","severity":1,"message":"119","line":2,"column":8,"nodeType":"116","messageId":"117","endLine":2,"endColumn":18},{"ruleId":"114","severity":1,"message":"120","line":4,"column":9,"nodeType":"116","messageId":"117","endLine":4,"endColumn":13},{"ruleId":"114","severity":1,"message":"121","line":10,"column":8,"nodeType":"116","messageId":"117","endLine":10,"endColumn":14},"no-native-reassign",["122"],"no-negated-in-lhs",["123"],"no-unused-vars","'FooterContact' is defined but never used.","Identifier","unusedVar","'startLoginWithGoogle' is defined but never used.","'SERVER_URL' is defined but never used.","'Link' is defined but never used.","'Button' is defined but never used.","no-global-assign","no-unsafe-negation"]